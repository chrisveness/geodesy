{
    "name": "geodesy",
    "description": "Libraries of geodesy functions",
    "homepage": "http://www.movable-type.co.uk/scripts/latlong.html",
    "repository": { "type": "git", "url": "https://github.com/chrisveness/geodesy" },
    "keywords": [ "geodesy", "latitude", "longitude", "distance", "bearing", "haversine", "vincenty", "rhumb", "ellipsoid", "datum", "wgs84", "utm", "mgrs" ],
    "author": "Chris Veness",
    "version": "1.0.2",
    "license": "MIT",
    "main": "npm.js",
    "bugs": "https://github.com/chrisveness/geodesy/issues",
    "scripts": {
        "test": "tape test.js | tap-spec",
        "lint": "eslint ."
    },
    "devDependencies": {
        "eslint": "^1.1.0",
        "jsdoc": "^3.3.2",
        "npm-check": "v3.2.10",
        "npm-check-updates": "v1.5.1",
        "tap-spec": "^4.0.2",
        "tape": "^4.0.3"
    },
    "testling": {
        "files": "test.js",
        "browsers": [
            "iexplore/7..latest",
            "chrome/26..latest",
            "firefox/21..latest",
            "safari/4..latest",
            "opera/11.5..latest",
            "iphone/6..latest",
            "ipad/6..latest",
            "android-browser/4.2..latest"
        ]
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "browser": true,
            "node": true,
            "amd": true
        },
        "rules": {
            "comma-dangle": [ 2, "always-multiline" ],
            "curly": [ 2, "multi-line" ],
            "indent": [ 2, 4, { "SwitchCase": 1 } ],
            "key-spacing": [ 1, { "align": "value" } ],
            "no-console": 1,
            "no-redeclare": 1,
            "no-unused-vars": 1,
            "object-curly-spacing": [ 2, "always" ],
            "quotes": [ 2, "single", "avoid-escape" ],
            "strict": [ 2, "global" ]
        }
    }
}
